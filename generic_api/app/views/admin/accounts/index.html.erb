<h1><%= t Account.model_name.human(count: 2) %></h1>
<table class="table table-hover">
  <thead>
    <tr>
      <th><%= Account.human_attribute_name(:id) %></th>
      <th><%= Account.human_attribute_name(:email) %></th>
      <th><%#= Account.human_attribute_name(:provider) %></th>
      <th><%#= Account.human_attribute_name(:uid) %></th>
      <th><%#= Account.human_attribute_name(:confirmation_token) %></th>
      <th><%#= Account.human_attribute_name(:confirmed_at) %></th>
      <th><%#= Account.human_attribute_name(:confirmation_sent_at) %></th>
      <th><%= Account.human_attribute_name(:name) %></th>
      <th><%= Account.human_attribute_name(:nickname) %></th>
      <th><%#= Account.human_attribute_name(:image) %></th>
      <th><%#= Account.human_attribute_name(:tokens) %></th>
      <th><%= t 'actions.actions' %></th>
    </tr>
  </thead>
  <tbody>
    <% @accounts.each do |account|  %>%
      <tr>
        <td><%= account.id %></td>
        <td><%= account.email %></td>
        <td><%#= account.provider %></td>
        <td><%#= account.uid %></td>
        <td><%#= account.confirmation_token %></td>
        <td><%#= account.confirmed_at %></td>
        <td><%#= account.confirmation_sent_at %></td>
        <td><%= account.name %></td>
        <td><%= account.nickname %></td>
        <td><%#= account.image %></td>
        <td><%#= account.tokens %></td>
        <td><%= link_to t('actions.show'), admin_account_path(account), class: 'btn btn-primary btn-sm' %></td>
        <td><%#= render 'admin/shared/index_buttons', resource: account %></td>
      </tr>
    <% end %>
  </tbody>
</table>
